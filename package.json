{
  "name": "deuna-msa-bo-mc-third-party-integrations",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "test": "jest --config ./jest.config.js --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "start": "nest start",
    "start:dev": "LOCAL=true nest start",
    "start:dev:windows": "set LOCAL=true && nest start",
    "test:ci": "jest --ci --config ./jest.config.js --detectOpenHandles --no-watchAll --runInBand",
    "typeorm": "node --conditions=typescript -r ts-node/register node_modules/typeorm/cli.js",
    "typeorm:migrate": "LOCAL=true npm run typeorm -- migration:run -c bo-mc-association-db",
    "typeorm:revert": "LOCAL=true npm run typeorm -- migration:revert -c bo-mc-association-db",
    "typeorm:generate": "LOCAL=true npm run typeorm -- migration:generate  -c bo-mc-association-db -n ",
    "typeorm:create": "LOCAL=true npm run typeorm -- migration:create  -c bo-mc-association-db -n ",
    "seed": "ts-node node_modules/typeorm-seeding/dist/cli.js seed -c bo-mc-association",
    "build": "nest build"
  },
  "dependencies": {
    "@deuna/node-commons": "^0.0.419133-master",
    "@deuna/node-environments": "^0.0.396597-nestjs10",
    "@deuna/node-logger": "^0.0.418985-master",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.3",
    "@nestjs/platform-express": "^10.2.3",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.16",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.3",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "keywords": [],
  "author": "",
  "private": true
}
